# Create-Class
public class NewPerson {
       String name;
       int age;
       //Constructor
       public NewPerson() {
       name = "Iris Bencomo";
       age = 19;
       }
       public NewPerson(String name, int age) {
       this.name = name;
       }
       //Getters
       public String getName() {
       return name;
       }
       //Setters
       public void setName(String name) {
       this.name = name;
       }
       public int getAge() {
       return age;
       }
       public void setAge() {
       this.age = age;
       }
       public String toString() {
       return this.name + "," + this.age;
       }
       public static void main(String[] args) {
       }
    }

