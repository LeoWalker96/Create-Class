# Create-Class
public class Person {
     String name;
     int age;
     //Constructor
     public Person() {
          name = "Leo Walker";
          age = 19;
     }
     public Person(String name, int age) {
          this.name = name;
     }
     //Getters
     public String getName() {
          return name;
     }
     public void setName(String name) {
          this.name = name;
     }
     public int getAge() {
          return age;
     }
     public void setAge(int age) {
          this.age = age;
     }
     public String toString() {
          return this.name + "," + this.age;
     }
     public static void main(String[] args) {
         
 }
